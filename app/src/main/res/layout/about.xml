<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
        android:layout_height="match_parent"
 android:background="#450100"
        android:visibility="visible"
        android:id="@+id/home"
        >

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true">


            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/scrollView4"
                android:layout_margin="15dp"
                android:layout_gravity="center_horizontal">

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" >

                    <ImageView
                        android:id="@+id/imageView5"
                        android:layout_width="150dp"
                        android:layout_height="150dp"
                        android:layout_gravity="center_horizontal"
                        android:scaleType="centerCrop"
                        app:srcCompat="@drawable/logo" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:id="@+id/textView162"
                        android:textColor="#ffffff"
                        android:layout_gravity="center_horizontal"
                        android:onClick="a" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:id="@+id/textView132"
                        android:layout_gravity="center_horizontal"
                        android:textColor="#ffff"
                        android:textSize="14dp"
                        android:layout_margin="4dp"
                        android:textStyle="normal"
                        android:text="The UPang Civil Engineering Reviewer was developed for UPang&apos;s Civil Engineering students who are preparing for the professional examination for teachers.  Using a huge database of questions built from review materials compiled over the years by UPang&apos;s College of Engineering and Architecture, the app simulates a mock examination of the licensure examination&apos;s subjects.  It also provides a simple but helpful assessment of the user&apos;s performance in an exam, thus persuading the user to intensify his/her review if needed." />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:id="@+id/textView135"
                        android:layout_gravity="center_horizontal"
                        android:textColor="#111111" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:id="@+id/textView150"
                        android:layout_gravity="center_horizontal"
                        android:textColor="#ffffff"
                        android:text="UPang Civil Engineering Reviewer was a project of the University of Pangasinan College of Information Technology Education, in close coordination with the College of Engineering and Architecture.  The app&apos;s developers are Joshua Nicole Angelo A. Martinez, Ronald Jesus Jimenez, Jayson Bascuel Quiniones, Cherry Vimes Monis and Deo Patrick Mendoza."
                        android:textStyle="normal"
                        android:textSize="16dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:id="@+id/textView155"
                        android:layout_gravity="center_horizontal"
                        android:textColor="#ffffff" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:id="@+id/textView157"
                        android:layout_gravity="center_horizontal"
                        android:textColor="#ffffff" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:id="@+id/textView159"
                        android:layout_gravity="center_horizontal"
                        android:textColor="#ffffff" />
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="300dp"
                        android:layout_gravity="left"
                        android:layout_height="match_parent"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:id="@+id/drawer"
                        >


                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <LinearLayout
                                android:orientation="horizontal"
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:layout_weight="0.24"
                                android:weightSum="1">

                                <LinearLayout
                                    android:orientation="horizontal"
                                    android:layout_width="120dp"
                                    android:layout_height="match_parent"
                                    android:weightSum="1"
                                    android:background="#450100">

                                    <ImageView
                                        android:layout_width="120dp"
                                        android:layout_height="match_parent"
                                        android:id="@+id/imageView16"
                                        android:src="@drawable/logo" />
                                </LinearLayout>

                                <LinearLayout
                                    android:orientation="horizontal"
                                    android:layout_width="270dp"
                                    android:layout_height="match_parent"
                                    android:weightSum="1"
                                    android:background="#450100" >

                                    <LinearLayout
                                        android:orientation="vertical"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content">

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:textAppearance="?android:attr/textAppearanceMedium"
                                            android:id="@+id/textView93"
                                            android:layout_gravity="center_horizontal" />

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:textAppearance="?android:attr/textAppearanceMedium"
                                            android:id="@+id/textView99"
                                            android:layout_gravity="center_horizontal" />

                                        <LinearLayout
                                            android:orientation="vertical"
                                            android:layout_width="215dp"
                                            android:layout_height="73dp"
                                            android:layout_gravity="center_horizontal|left">

                                            <TextView
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:textAppearance="?android:attr/textAppearanceMedium"
                                                android:id="@+id/textView98"
                                                android:layout_gravity="center_horizontal"
                                                android:textColor="#ffa401"
                                                android:textIsSelectable="false"
                                                android:textSize="9.5dp"
                                                android:textStyle="bold" />

                                            <TextView
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:textAppearance="?android:attr/textAppearanceMedium"
                                                android:id="@+id/textView38"
                                                android:layout_gravity="center_horizontal"
                                                android:textColor="#ffa401"
                                                android:textIsSelectable="false"
                                                android:textSize="9.5dp"
                                                android:textStyle="bold" />

                                            <TextView
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:textAppearance="?android:attr/textAppearanceMedium"
                                                android:text="UNIVERSITY OF PANGASINAN - PHINMA"
                                                android:id="@+id/textView95"
                                                android:layout_gravity="center_horizontal|left"
                                                android:textColor="#ffa401"
                                                android:textIsSelectable="false"
                                                android:textSize="9.5dp"
                                                android:textStyle="bold" />

                                            <LinearLayout
                                                android:orientation="vertical"
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:layout_gravity="center_horizontal" >

                                                <TextView
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:textAppearance="?android:attr/textAppearanceMedium"
                                                    android:text="      CIVIL ENGINEERING REVIEWER"
                                                    android:id="@+id/textView123"
                                                    android:layout_gravity="center_horizontal|left"
                                                    android:textColor="#ffa401"
                                                    android:textIsSelectable="false"
                                                    android:textSize="10dp"
                                                    android:textStyle="bold" />

                                            </LinearLayout>
                                        </LinearLayout>

                                        <LinearLayout
                                            android:orientation="vertical"
                                            android:layout_width="match_parent"
                                            android:layout_height="48dp" >

                                            <TextView
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:textAppearance="?android:attr/textAppearanceMedium"
                                                android:id="@+id/textView152"
                                                android:layout_gravity="center_horizontal" />

                                            <LinearLayout
                                                android:orientation="vertical"
                                                android:layout_width="wrap_content"
                                                android:layout_height="34dp"
                                                android:layout_gravity="center_horizontal|left" >

                                                <LinearLayout
                                                    android:orientation="vertical"
                                                    android:layout_width="match_parent"
                                                    android:layout_height="wrap_content"
                                                    android:layout_gravity="center_horizontal" >

                                                    <TextView
                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:textAppearance="?android:attr/textAppearanceMedium"
                                                        android:id="@+id/textView151"
                                                        android:layout_gravity="center_horizontal|left"
                                                        android:textColor="#ffa401"
                                                        android:textIsSelectable="false"
                                                        android:textSize="10dp"
                                                        android:textStyle="bold" />
                                                </LinearLayout>
                                            </LinearLayout>
                                        </LinearLayout>
                                    </LinearLayout>

                                    <LinearLayout
                                        android:orientation="vertical"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content" >

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:textAppearance="?android:attr/textAppearanceMedium"
                                            android:id="@+id/textView90"
                                            android:layout_gravity="center_horizontal" />

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:textAppearance="?android:attr/textAppearanceMedium"
                                            android:id="@+id/textView92"
                                            android:layout_gravity="center_horizontal" />

                                        <LinearLayout
                                            android:orientation="vertical"
                                            android:layout_width="210dp"
                                            android:layout_height="34dp"
                                            android:layout_gravity="center_horizontal|left" >

                                            <TextView
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:textAppearance="?android:attr/textAppearanceMedium"
                                                android:text="UNIVERSITY OF PANGASINAN - PHINMA"
                                                android:id="@+id/textView133"
                                                android:layout_gravity="center_horizontal|left"
                                                android:textColor="#ffa401"
                                                android:textIsSelectable="false"
                                                android:textSize="9.5dp"
                                                android:textStyle="bold" />

                                            <LinearLayout
                                                android:orientation="vertical"
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:layout_gravity="center_horizontal" >

                                                <TextView
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:textAppearance="?android:attr/textAppearanceMedium"
                                                    android:text="      CIVIL ENGINEERING REVIEWER"
                                                    android:id="@+id/textView134"
                                                    android:layout_gravity="center_horizontal|left"
                                                    android:textColor="#ffa401"
                                                    android:textIsSelectable="false"
                                                    android:textSize="10dp"
                                                    android:textStyle="bold" />
                                            </LinearLayout>
                                        </LinearLayout>
                                    </LinearLayout>
                                </LinearLayout>

                            </LinearLayout>

                        </LinearLayout>
                    </LinearLayout>

                </LinearLayout>

            </ScrollView>

        </LinearLayout>
    </RelativeLayout>

